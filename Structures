};
Student s;
{printf("enter the name and usn of the sudent\n");
 scanf("%s%s",&s.name,&s.usn);
 printf("enter marks obtained in all 6 subs\n");
 for(i=0;i<6;i++)
  scanf("%d",&mark[i]);
 printf("printing the info of the required Student\n");
 printf("Name :%s\nUSN :%s",s.name,s.usn\n);
